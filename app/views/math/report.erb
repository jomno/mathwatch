<table>
  <thead>
    <th>문항 번호</th>
    <th>정답</th>
    <th>해설 보기</th>
  </thead>
  <tbody>
    <% @data.each_with_index do |d,index|%>
    <tr>
      <td>
        <%= index+1%>
      </td>
      <td>
        <%= d.correct %>
      </td>
      <td>
        <a href="#">해설</a>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
